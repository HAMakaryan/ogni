onerror {resume}
radix define I2C_CMD {
  "3'b000"          "START_CMD"   -color #F1F10B,
  "3'b001"          "WR_CMD"      -color #89F10B,
  "3'b010"          "RD_CMD"      -color #0BF1CB,
  "3'b011"          "STOP_CMD"    -color #0B93F1,
  "3'b100"          "RESTART_CMD" -color #F7DC6F,
  -default hexadecimal
}




